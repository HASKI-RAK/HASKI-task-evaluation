    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.16",
    "litegraph.js": "^0.7.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "react-use-websocket": "^4.5.0",
    "zustand": "^4.4.6"
