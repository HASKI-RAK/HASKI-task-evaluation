    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-pwa": "^0.16.5",
    "vite-tsconfig-paths": "^4.2.1",
